{
  "name": "yazi-ai.git",
  "version": "1.0.2",
  "description": "Yazi âœ¨ is a chatbot with some useful functions.",
  "keywords": [
    "typescript"
  ],
  "author": {
    "name": "Gabriel Maia",
    "url": "https://github.com/gabrielmaialva33"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielmaialva33/yazi-ai.git"
  },
  "private": false,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/main.js",
    "start:build": "rimraf dist && tsc && node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/src/main.js",
    "start:dev": "nodemon",
    "start:test": "nodemon --watch './**/*.ts' --exec 'ts-node' bin/test.ts",
    "test": "ts-node bin/test.ts",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ],
      "@typescript-eslint/naming-convention": "off"
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-transform-typescript": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/luxon": "^3.2.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "babel-plugin-tsconfig-paths-module-resolver": "^1.0.4",
    "chalk": "^4.1.2",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "openapi-types": "^12.1.0",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.2",
    "yarn-upgrade-all": "^0.7.2"
  },
  "dependencies": {
    "@types/mime-types": "^2.1.1",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "jimp": "^0.22.7",
    "luxon": "^3.3.0",
    "openai": "^3.2.1",
    "venom-bot": "^4.3.7"
  }
}
